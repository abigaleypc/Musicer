{
  "name": "musicer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel electron webpack:watch",
    "serve": "nodemon index.js",
    "electron": "electron ./public/main.js",
    "webpack": "webpack",
    "webpack:watch": "npm run webpack -- --watch",
    "test": "mocha -R nyan",
    "prepackage": "npm run webpack",
    "package": "electron-packager ./ musicer --win --out build --arch=x64 --version 1.7.9 --overwrite --ignore=node_modules --icon=public/assets/musicer.icns",
    "prepackage:win": "npm run webpack",
    "package:win": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=build --icon=public/assets/musicer.ico",
    "clean": "rimraf build && rimraf public/dist && rimraf src/data"
  },
  "main": "./public/main.js",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "css-loader": "^0.28.7",
    "electron-packager": "^9.1.0",
    "electron-prebuilt": "^1.4.13",
    "electron-reload": "^1.2.2",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.4.0",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash.isequal": "^4.5.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.1",
    "react-dev-utils": "^5.0.1",
    "request-debug": "^0.2.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "electron": "^2.0.0",
    "express": "~4.15.2",
    "font-awesome": "^4.7.0",
    "fs": "0.0.1-security",
    "moment": "^2.20.1",
    "qrcode": "^1.0.0",
    "react": "^16.0.0",
    "react-color": "^2.13.8",
    "react-dom": "^16.0.0",
    "react-native-video": "^2.0.0",
    "react-redux": "^5.0.6",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "request": "^2.83.0"
  }
}
